{
  "name": "auto-devil",
  "description": "A simple web-app for robot path planning",
  "homepage": "auto.devilbots.org",
  "author": "DevilBots",
  "private": true,
  "version": "0.3.0",
  "main": "./out/main/main.js",
  "license": "GPL-3.0",
  "scripts": {
    "dev": "electron-vite dev -w",
    "build": "tsc --noEmit --composite false && electron-vite build",
    "build:win": "yarn run build && electron-builder --win",
    "build:mac": "yarn run build && electron-builder --mac",
    "build:linux": "yarn run build && electron-builder --linux",
    "build:test": "yarn run build && electron-builder --dir",
    "postinstall": "electron-builder install-app-deps"
  },
